<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/activity_main"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="wang.androids.plugindev.modulea.MainActivity">

    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:id="@+id/ahello"
        android:text="Hello World!" />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:id="@+id/astartmain"
        android:text="a start module main" />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
    android:id="@+id/astartb"
    android:text="a start module b" />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:id="@+id/AqueryMain"
        android:text="AqueryMain" />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:id="@+id/AaddtoMain"
        android:text="A add to Main" />

</LinearLayout>
